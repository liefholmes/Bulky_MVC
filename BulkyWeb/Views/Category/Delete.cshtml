@model Category

<h2>Are you sure you want to delete this?</h2>

<div>
    <h4>@Model.Name</h4>
    <form method="post">
        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
